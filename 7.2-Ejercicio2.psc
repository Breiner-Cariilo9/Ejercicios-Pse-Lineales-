algoritmo cacular_notas_materias
 definir materia1, materia2, materia3, suma como real;
escribir "Digite la nota de la materia 1 ";
leer materia1;
escribir "Digite la nota de la materia 2 ";
leer materia2;
escribir "Digite la nota de la materia 3 ";
leer materia3;
suma <- (materia1 + materia2 + materia3)/3;
mostrar "El promedio de sus notas es de ";
escribir suma;
finalgoritmo
